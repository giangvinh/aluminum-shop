block info
    script.
        var page_name = "notification_page";



block header
    header.page-header
        h2 Tổng quan
        div(class="right-wrapper pull-right")
            ol.breadcrumbs
                li
                    a(href="dashboard")
                        i(class="fa fa-home")
                li
                    span Tổng quan

            a(class="sidebar-right-toggle" data-open="sidebar-right")
                i(class="fa fa-chevron-left")

block content

    // start: page
    .inner-toolbar.clearfix
        ul

            li.right
                ul.nav.nav-pills.nav-pills-primary
                    li
                        label Type
                    li.active
                        a(href='#access-log', data-toggle='tab') Access Log
                    li
                        a(href='#error-log', data-toggle='tab') Error Log
                    li
                        a(href='#custom-log', data-toggle='tab') Custom Log
    section.panel
        .panel-body.tab-content
            #access-log.tab-pane.active
                table.table.table-striped.table-no-more.table-bordered.mb-none
                    thead
                        tr
                            th(style='width: 10%')
                                span.text-normal.text-sm Type
                            th(style='width: 15%')
                                span.text-normal.text-sm Date
                            th
                                span.text-normal.text-sm Message
                    tbody.log-viewer
                        each obj in notification
                            tr
                                td.pt-md.pb-md(data-title='Type')
                                    i(class=obj.icon)
                                td.pt-md.pb-md(data-title='Date')
                                    | #{obj.created_date}
                                td.pt-md.pb-md(data-title='Message')
                                    | #{obj.title}

            #error-log.tab-pane
                table.table.table-striped.table-no-more.table-bordered.mb-none
                    thead
                        tr
                            th(style='width: 10%')
                                span.text-normal.text-sm Type
                            th(style='width: 15%')
                                span.text-normal.text-sm Date
                            th
                                span.text-normal.text-sm Message

            #custom-log.tab-pane
                table.table.table-striped.table-no-more.table-bordered.mb-none
                    thead
                        tr
                            th(style='width: 10%')
                                span.text-normal.text-sm Type
                            th(style='width: 15%')
                                span.text-normal.text-sm Date
                            th
                                span.text-normal.text-sm Message
                    tbody.log-viewer





